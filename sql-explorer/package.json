{
  "name": "sql-explorer",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "node app.js",
    "backup:db:with-data": "node database/backupDB.js export --with-data",
    "backup:db:without-data": "node database/backupDB.js export --without-data",
    "db:setup": "node database/backupDB.js import database/backup/initial_backup.sql",
